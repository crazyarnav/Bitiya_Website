@model List<BITIYA_WebSite.Models.WowSliderClass>

@{
    ViewBag.Title = "trywow";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int i = 0;
    string wowstr = "wows1_";
    string str = wowstr;
    
}
@*<title>WOWSlider generated by WOWSlider.com</title>*@
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<meta name="keywords" content="WOW Slider, Slideshow Maker HTML, Code For Slideshow In HTML" />
<meta name="description" content="WOWSlider created with WOW Slider, a free wizard program that helps you easily generate beautiful web slideshow" />
<!-- Start WOWSlider.com HEAD section -->
<link rel="stylesheet" type="text/css" href="../../Wowslider/Events/HolmdelWalkathon/engine1/style.css?v=1.1" />
@*<script type="text/javascript">
    jQuery.noConflict();
    var $j = jQuery;
</script>*@
<script type="text/javascript" src="../../Wowslider/Events/HolmdelWalkathon/engine1/jquery.js"></script>
<!-- End WOWSlider.com HEAD section -->
<div id="MainContent">
<br />
    <!-- Start WOWSlider.com BODY section -->
    <div id="wowslider-container1">
        <div class="ws_images">
  
            <ul runat="server" id="ulimages">
                @foreach (var n in Model)
                {
                    str = wowstr + i.ToString();
                    i++;             
                    <li><img src="@ViewBag.imagePath@n.imageName" alt="@n.imageName" title="" id="@str"/></li>
                }
            </ul>
        </div>
        <div class="ws_bullets" >
            <div>
                @foreach (var tt in Model)
                { 
                    <a href="#" title="@tt.ttName"> <img src="@ViewBag.ttPath@tt.ttName" alt="@tt.ttName">@i</a>
                    i++;
                }
            </div>
        </div>
    
        <a class="wsl" href="http://wowslider.com">Slideshow For HTML by WOWSlider.com v2.5</a>
        <div class="ws_shadow"></div>
</div>
    <script type="text/javascript" src="../../Wowslider/Events/HolmdelWalkathon/engine1/wowslider.js"></script>
    <script type="text/javascript" src="../../Wowslider/Events/HolmdelWalkathon/engine1/script.js"></script>
    <!-- End WOWSlider.com BODY section -->
    <!-- </body> -->
</div>
